<template name="listTmp">
  <view class="tmpContainer">
    <view class="tmpAvatar">
      <image src="{{avatar}}"></image>
      <text>{{date}}</text>
    </view>
    <text class="company">{{title}}</text>
    <image class="contentImg" src="{{detail_img}}"></image>
    <!-- 
      样式text-indent: 64rpx;
      在text标签下无效，在view标签中有效
      text标签，回车键可以让文本自动换行
      view标签，不能回车键换行，也不能\n换行
     -->
    <text class="content">
      {{detail_content}}
    </text>
    <view class="collection">
      <image src="/images/wt(1).png"></image>
      <text>{{love_count}}</text>
      <image src="/images/wt-active(1).png"></image>
      <text>{{attention_count}}</text>
    </view>
  </view>
</template>